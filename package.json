  "scripts": {
    "coverage": "NODE_ENV=test istanbul --include-all-sources cover _mocha  -- static/src/js/test --compilers js:babel/register --recursive && cat ./coverage/coverage.json | ./node_modules/codecov.io/bin/codecov.io.js",
    "coverage-coveralls": "NODE_ENV=test istanbul --include-all-sources cover _mocha --report lcovonly  -- static/src/js/test --compilers js:babel/register --recursive && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "test": "NODE_ENV=test mocha static/src/js/test --opts static/src/js/test/mocha.opts",
    "devserver": "NODE_ENV=development webpack-dev-server --progress --hot --config webpack-development.config.js",
    "webpack": "NODE_ENV=production webpack --optimize-minimize --optimize-dedupe --optimize-occurence-order --colors --progress --config webpack-production.config.js",
    "install_global_modules": "npm install -g istanbul webpack webpack-dev-server eslint eslint-plugin-react mocha"
  }
